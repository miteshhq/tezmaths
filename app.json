{
    "expo": {
        "name": "TezMaths",
        "slug": "tezmaths",
        "version": "1.0.1",
        "orientation": "portrait",
        "icon": "./assets/branding/main-logo.png",
        "scheme": "tezmaths",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.tezmathsteam.tezmaths",
            "googleServicesFile": "./GoogleService-Info.plist",
            "buildNumber": "2"
        },
        "android": {
            "jsEngine": "jsc",
            "adaptiveIcon": {
                "foregroundImage": "./assets/branding/main-logo.png",
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true,
            "package": "com.tezmathsteam.tezmaths",
            "googleServicesFile": "./google-services.json",
            "versionCode": 2
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/branding/main-logo.png"
        },
        "plugins": [
            "expo-router",
            "expo-dev-client",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/branding/main-logo.png",
                    "imageWidth": 250,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ],
            "expo-audio"
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {},
            "eas": {
                "projectId": "9f26400b-95d7-47d7-a612-3a06f29576c6"
            }
        }
    }
}